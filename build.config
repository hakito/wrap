# Target filesystem type [ext2, ext3, ext4]
FILESYSTEM=ext4

# Path to kernel src files
KERNELSRC=linux

# Size of swarp in MB
SWAPSIZE=256

# Ubuntu version name
UBUNTUVERSION=trusty

# Serial console baud
BAUD=57600

# Additional APT packages
PACKAGES="isc-dhcp-client openssh-server net-tools"

# Newusers (name:password:uid:gid:gecos:home:shell)
NEWUSERS="wrap:wrap:::wrap:/home/wrap:/bin/bash"
